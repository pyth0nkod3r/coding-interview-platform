{
  "name": "coding-interview-backend",
  "version": "1.0.0",
  "description": "Fastify backend for Coding Interview Platform",
  "type": "module",
  "main": "dist/server.js",
  "scripts": {
    "dev": "tsx watch src/server.ts",
    "build": "tsc",
    "start": "node dist/server.js",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:integration": "rm -f prisma/test.db && vitest run --config vitest.config.integration.ts",
    "verify": "tsx scripts/verify_api.ts",
    "db:generate": "prisma generate --schema=prisma/schema.sqlite.prisma",
    "db:push": "prisma db push --schema=prisma/schema.sqlite.prisma",
    "db:studio": "prisma studio --schema=prisma/schema.sqlite.prisma",
    "db:generate:pg": "prisma generate --schema=prisma/schema.prisma",
    "db:push:pg": "prisma db push --schema=prisma/schema.prisma",
    "db:migrate:pg": "prisma migrate dev --schema=prisma/schema.prisma"
  },
  "dependencies": {
    "@fastify/cors": "^11.2.0",
    "@fastify/jwt": "^10.0.0",
    "@fastify/swagger": "^9.6.1",
    "@fastify/swagger-ui": "^5.2.3",
    "@fastify/websocket": "^11.2.0",
    "@prisma/client": "5.22.0",
    "fastify": "^5.6.2",
    "prisma": "5.22.0"
  },
  "devDependencies": {
    "@types/estree": "^1.0.8",
    "@types/node": "^20.19.26",
    "tsx": "^4.7.0",
    "typescript": "^5.3.0",
    "vite": "^5.4.11",
    "vitest": "^2.1.8"
  }
}
