{
  "name": "coding-interview-backend",
  "version": "1.0.0",
  "description": "Fastify backend for Coding Interview Platform",
  "type": "module",
  "main": "dist/server.js",
  "scripts": {
    "dev": "tsx watch src/server.ts",
    "build": "tsc",
    "start": "node dist/server.js",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:integration": "vitest run --config vitest.config.integration.ts",
    "verify": "tsx scripts/verify_api.ts"
  },
  "dependencies": {
    "@fastify/cors": "^9.0.1",
    "@fastify/jwt": "^8.0.1",
    "@fastify/swagger": "^8.15.0",
    "@fastify/swagger-ui": "^2.1.0",
    "@prisma/client": "5.22.0",
    "fastify": "^4.28.1",
    "prisma": "5.22.0"
  },
  "devDependencies": {
    "@types/estree": "^1.0.8",
    "@types/node": "^20.19.26",
    "tsx": "^4.7.0",
    "typescript": "^5.3.0",
    "vitest": "^1.6.0"
  }
}